{
  "name": "tactical-shooter-5v5",
  "version": "1.0.0",
  "description": "Rainbow Six Siege-style tactical shooter for web",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "vite",
    "build": "vite build",
    "start": "node server/server.js",
    "render-build": "npm install && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@babylonjs/core": "^6.0.0",
    "@babylonjs/loaders": "^6.0.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.2.0"
  }
}
